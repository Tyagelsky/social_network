<% if @feed_items.any? %>
  <ol class="microposts">
    <li id="<%= feed_item.id %>">
      <%= image_tag feed_item.user.avatar.url(:thumb) %>
    <span class="user">
    <%= link_to feed_item.user.username, feed_item.user %>
  </span>
  <span class="body"><%= feed_item.body %></span>
  <span class="timestamp">
    Posted <%= time_ago_in_words(feed_item.created_at) %> ago.
  </span>
</li>
  </ol>

<% end %>
